{
	"$schema": "vscode://schemas/color-theme",
	"name": "Laravel Theme",
	"type": "light",
	"colors": {
		"editor.background": "#ffffff",
		"editor.foreground": "#24292e",
		"editorLineNumber.foreground": "#ccc",
		"editorIndentGuide.background": "#eee",
		"editorBracketMatch.background": "#f1f8ff",
		"editorBracketMatch.border": "#c8e1ff",
		"editor.lineHighlightBackground": "#ecebeb",
		"focusBorder": "#fafbfc",

		"sideBar.background": "#ffffff",
		"sideBar.foreground": "#586069",
		"sideBar.border": "#ffffff",
		"sideBarTitle.foreground": "#24292e",
		"sideBarSectionHeader.background": "#ffffff",
		"sideBarSectionHeader.foreground": "#555",

		"statusBar.foreground": "#24292e",
		"statusBar.background": "#ffffff",
		"statusBar.border": "#ffffff",
		"statusBar.noFolderForeground": "#24292e",
		"statusBar.noFolderBackground": "#fafbfc",
		"statusBar.debuggingForeground": "#24292e",
		"statusBar.debuggingBackground": "#fafbfc",

		"notification.background": "#54a7ff"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#333333ff",
				"background": "#f5f5f5ff"
			}
		},
		{
			"name": "Source",
			"scope": [
				"source"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#999"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#999"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation",
				"string.quoted.docstring.multi.python",
				"punctuation.definition.string.begin.python",
				"punctuation.definition.string.end.python"
			],
			"settings": {
				"foreground": "#999"
			}
		},
		{
			"name": "Invalid - Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"background": "#96000014"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#96000014",
				"foreground": "#DA564A"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator",
				"keyword.operator.class.php",
				"keyword.operator.key.php"
			],
			"settings": {
				"foreground": "#555"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"support.class",
				"entity.name.function",
				"entity.name.type.class",
				"entity.other.inherited-class"

			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage",
				"storage.type.function.php",
				"storage.type.class.php",
				"keyword.operator.logical",
				"keyword.operator.expression.in.js",
				"entity.other.static-class.php"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#07a"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#07a"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"constant.numeric",
				"constant.other.php"
			],
			"settings": {
				"foreground": "#DA564A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"source.php variable",
				"variable.other.php"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Variables php",
			"scope": [
				"variable.other.property.php"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "this",
			"scope": [
				"variable.language.this"
			],
			"settings": {
				"foreground": "#4EA1DF"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#DA564A"
			}
		},
		{
			"name": "Sections",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Characters, $this php",
			"scope": [
				"constant.character",
				"constant",
				"variable.language",
				"constant.other.php"
			],
			"settings": {
				"foreground": "#222"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#2E7D32"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#2E7D32"
			}
		},
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#999"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#07a"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#4EA1DF",
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#DA564A"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"string.quoted.double.html",
				"string.quoted.single.html",
				"string.quoted.html"
			],
			"settings": {
				"foreground": "#2E7D32"
			}
		},
		{
			"name": "directivas blade",
			"scope": [
				"entity.name.function.laravel-blade"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DA564A"
			}
		},
		{
			"name": "blade {{  }}",
			"scope": [
				"support.constant.laravel-blade"
			],
			"settings": {
				"foreground": "#DA564A"
			}
		},

		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css",
				"punctuation.definition.entity.css",
				"constant.numeric.css"
			],
			"settings": {
				"foreground": "#07a"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name",
				"punctuation.terminator.rule.css",
				"punctuation.section.property-list.begin.bracket.curly.css",
				"punctuation.section.property-list.end.bracket.curly.css"
			],
			"settings": {
				"foreground": "#555"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#2E7D32"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"background": "#FFFFDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"background": "#96000014",
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#07a"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#DA564A"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#07a"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#2E7D32"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#07a"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"background": "#DDDDFF",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Json key",
			"scope": "support.type.property-name.json",
			"settings": {
				"foreground": "#07a"
			}
		},
		{
			"name": "Json key",
			"scope": "string.quoted.double.json",
			"settings": {
				"foreground": "#222"
			}
		},


		{
			"name": "Django Tag",
			"scope": [
				"keyword.control.django"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DA564A"
			}
		},
		{
			"name": "Django  {{}}",
			"scope": [
				"entity.other.django.delimiter.variable",
				"entity.other.django.delimiter.tag"
			],
			"settings": {
				"foreground": "#DA564A"
			}
		},
		{
			"name": "Python operator logical",
			"scope": [
				"keyword.operator.logical.python"
			],
			"settings": {
				"foreground": "#07a"
			}
		},
		{
			"name": "Python self",
			"scope": [
				"variable.language.special.self.python"
			],
			"settings": {
				"foreground": "#4EA1DF"
			}
		},





		{
			"name": "punctuation",
			"scope": [
				"source.py punctuation",
				"source.js punctuation"
			],
			"settings": {
				"foreground": "#555"
			}
		},
		{
			"name": "JS punctuation",
			"scope": [
				"meta.brace.square.js",
				"meta.brace.round.js"
			],
			"settings": {
				"foreground": "#555"
			}
		},
		{
			"name": "JS operator in",
			"scope": [
				"keyword.operator.expression.in.js",
				"keyword.operator.new.js",
				"keyword.operator.expression.typeof.js"
			],
			"settings": {
				"foreground": "#07a"
			}
		},

		{
			"name": "XML atribute",
			"scope": "entity.other.attribute-name.localname.xml",
			"settings": {
				"foreground": "#07a"
			}
		},
		{
			"name": "XML atribute",
			"scope": [
				"markup.raw.block.markdown",
				"text.html.markdown"
			],
			"settings": {
				"foreground": "#222"
			}
		}

	]
}
